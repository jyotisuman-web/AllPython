#prime number between interval
n=int(input("write the number between 900 and 1000 : "))
if n>0:
 for n in range(900,1000+1):
   for i in range(2,n):
        if n%i==0:
           #print(n,"is not a prime number")
           break
   else:
            print(n,"is a prime number")
          
else:
      # print(n,"is a  not prime number")
